<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set/" xmlns:role="http://www.sitecore.net/xmlconfig/role/">
    <sitecore role:require="Standalone OR ContentManagement OR ContentDelivery OR Processing OR Reporting">
        <events>
            <event name="item:saved">
                <handler type="Sitecore.Support.LanguageFallbackFieldValuesCache.ItemEventHandler, Sitecore.Support.211627" method="OnItemSaved" />
            </event>
            <event name="item:saved:remote">
                <handler type="Sitecore.Support.LanguageFallbackFieldValuesCache.ItemEventHandler, Sitecore.Support.211627" method="OnItemSavedRemote" />
            </event>
        </events>
    </sitecore>
</configuration>